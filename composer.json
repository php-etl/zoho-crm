{
    "name": "php-etl/zoho-crm-flow",
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\ZohoCRM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Kiboko\\Component\\Flow\\ZohoCRM\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "GrÃ©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "php-etl/pipeline-contracts": ">=0.4.0 <0.6",
        "php-etl/bucket": ">=0.3.0 <0.5",
        "psr/log": "^3.0",
        "psr/simple-cache": "^3.0",
        "php-etl/mapping-contracts": "0.4.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "php-etl/phpunit-extension": "*",
        "guzzlehttp/psr7": "^2.4",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "rector/rector": "^0.15.24"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
